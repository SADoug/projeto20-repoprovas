    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "jest": "^29.0.3",
    "prisma": "^4.3.1",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
